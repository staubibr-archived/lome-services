{
    "databaseChangeLog": [{
            "changeSet": {
                "id": "contributors_table",
                "comment": "Create contributors table",
                "author": "Hamza Qassoud",
                "preConditions": [{
                        "onFail": "MARK_RAN",
                        "not": {
                            "tableExists": {
                                "tableName": "contributors"
                            }
                        }
                    }
                ],
                "changes": [{
                        "createTable": {
                            "columns": [{
                                    "column": {
                                        "constraints": {
                                            "primaryKey": true
                                        },
                                        "name": "id",
                                        "type": "BIGINT",
                                        "autoIncrement": true
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "first_name",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "last_name",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "name": "middle_name",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "email",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "name": "affiliation",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "creation_date",
                                        "type": "DATE"
                                    }
                                }
                            ],
                            "tableName": "contributors"
                        }
                    }
                ]
            }
        }
    ]
}
