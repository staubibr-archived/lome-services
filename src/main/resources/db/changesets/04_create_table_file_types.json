{
    "databaseChangeLog": [{
            "changeSet": {
                "id": "file_types",
                "comment": "Create file types table",
                "author": "Bruno St-Aubin",
                "preConditions": [{
                        "onFail": "MARK_RAN",
                        "not": {
                            "tableExists": {
                                "tableName": "file_types"
                            }
                        }
                    }
                ],
                "changes": [{
                        "createTable": {
                            "columns": [{
                                    "column": {
                                        "constraints": {
                                            "primaryKey": true
                                        },
                                        "name": "id",
                                        "type": "BIGINT",
                                        "autoIncrement": true
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "description",
                                        "type": "VARCHAR(255)"
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "extension",
                                        "type": "VARCHAR(10)"
                                    }
                                }
                            ],
                            "tableName": "file_types"
                        }
                    }
                ]
            }
        }
    ]
}
