{
    "databaseChangeLog": [{
            "changeSet": {
                "id": "tags_table",
                "comment": "Create tags table",
                "author": "Bruno St-Aubin",
                "preConditions": [{
                        "onFail": "MARK_RAN",
                        "not": {
                            "tableExists": {
                                "tableName": "tags"
                            }
                        }
                    }
                ],
                "changes": [{
                        "createTable": {
                            "columns": [{
                                    "column": {
                                        "constraints": {
                                            "primaryKey": true
                                        },
                                        "name": "id",
                                        "type": "BIGINT",
                                        "autoIncrement": true
                                    }
                                }, {
                                    "column": {
                                        "constraints": {
                                            "nullable": false
                                        },
                                        "name": "value",
                                        "type": "VARCHAR(255)"
                                    }
                                }
                            ],
                            "tableName": "tags"
                        }
                    }
                ]
            }
        }
    ]
}
